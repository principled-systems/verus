(check-valid
 (declare-const tmp%1 Bool)
 (declare-var a@ Bool)
 (declare-var a_ref@ Proph%B)
 (axiom fuel_defaults)
 (block
  ; let mut a: Ghost<bool> = Ghost(true);
  (assume (= a@ true))

  ; let mut a_ref: &mut Ghost<nat> = &mut a;
  (assume (= (Proph%value%B a_ref@) a@))
  (havoc a@)
  (assume (= a@ (Proph%future%B a_ref@)))

  ; *a_ref = Ghost(!a@);
  (block
   (snapshot snap%ASSIGN)
   (havoc a_ref@)
   (assume (and
      (= (Proph%future%B (old snap%ASSIGN a_ref@)) (Proph%future%B a_ref@))
      (= (Proph%value%B a_ref@) (not a@))
   ))
  )

  ; finalize_mut_ref(a_ref) aka resolve(a_ref)
  (assume (= (Proph%value%B a_ref@) (Proph%future%B a_ref@)))
  (assert false)
))
